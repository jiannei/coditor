{
  "name": "@s2nc/coditor",
  "version": "0.3.0",
  "description": "a markdown editor based on milkdown.",
  "keywords": [
    "milkdown",
    "markdown",
    "editor",
    "milkdown editor"
  ],
  "author": "jiannei",
  "license": "MIT",
  "main": "./dist/coditor.umd.js",
